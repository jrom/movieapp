<h1>UserSessions#new</h1>
  
<% form_for @user_session do |form| %>  
	<%= form.error_messages %>  
  <div class = "field">  
    <%= form.label :username, "Username" %>  
    <%= form.text_field :username %>  
  </div>  
  <div class = "field">  
    <%= form.label :password, "Password" %>  
    <%= form.password_field :password %>  
  </div>  
  <div class = "field">  
    <%= form.submit "Submit" %>  
  </div>  
<% end %>
